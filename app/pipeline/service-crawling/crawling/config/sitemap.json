        "journey_id": "acts_in_force",
        "description": "Navigates to 'Acts in force' and scrapes results via pagination.",
        "steps": [
          { "action": "navigate", "target": "https://www.legislation.vic.gov.au/" },
          { "action": "click", "target": { "type": "xpath", "value": "//*[@id='page-component-101686']/div/div[2]/h3/a" }, "post_action_wait_seconds": 3 },
