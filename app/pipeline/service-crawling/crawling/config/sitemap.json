          { "action": "click", "description": "Click apply filter to load initial results.", "target": { "type": "xpath", "value": "//button[@id='tide-search-filter-form-actions']" }, "post_action_wait_seconds": 5 },
          {
            "action": "numeric_pagination_loop",
            "description": "Loop through all result pages by clicking numbers.",
            "page_number_xpath_template": "//nav//ol//button[normalize-space()='{page_num}']",
            "next_button_fallback_xpath": "//nav//button[last()]",
            "post_action_wait_seconds": 3,
            "loop_steps": [
              { "action": "process_results", "description": "Scrape results from the current page.", "target": { "type": "xpath", "value": "//div[contains(@class, 'search-results')]" } }
