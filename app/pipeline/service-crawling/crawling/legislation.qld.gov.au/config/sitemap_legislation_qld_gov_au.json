            "loop_steps": [
              {
                "action": "next_button_pagination_loop",
                "next_button_xpath": "//li[@id='table_2_next' and not(contains(@class, 'disabled'))]",
                "loop_steps": [
                  {
                    "action": "process_results",
                    "target": {
                      "type": "xpath",
                      "value": "//*[@id='table_2']"
                    },
                    "scraping_config": {
                      "row_xpath": "./tbody/tr",
                      "columns": [
                        {
                          "name": "title",
                          "xpath": ".//td[1]/a",
                          "type": "text"
                        },
                        {
                          "name": "link",
                          "xpath": ".//td[1]/a",
                          "type": "href"
                        },
                        {
                          "name": "effective_date",
                          "xpath": ".//td[2]",
                          "type": "text"
                        },
                        {
                          "name": "year",
                          "xpath": ".//td[3]",
                          "type": "text"
                        },
                        {
                          "name": "number",
                          "xpath": ".//td[4]",
                          "type": "text"
                        }
                      ]
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
